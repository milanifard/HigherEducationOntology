    <rdfs:comment>پایان نامه</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ScientificProduct"/>
  </owl:Class>
  <owl:Class rdf:ID="paper">
    <rdfs:comment>مقاله</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ScientificProduct"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#presentedAt" />
        <owl:someValuesFrom rdf:resource="#event" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#presentedAt" />
        <owl:someValuesFrom rdf:resource="#MagazineIssue" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="book">
    <rdfs:comment>کتاب</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ScientificProduct"/>
  </owl:Class>
  <owl:Class rdf:ID="patent">
    <rdfs:comment>اختراع</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ScientificProduct"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="BirthAt">
    <rdfs:comment>متولد شدن در</rdfs:comment>
    <rdfs:domain rdf:resource="#person"/>
    <rdfs:range rdf:resource="#country"/><rdfs:range rdf:resource="#city"/><rdfs:range rdf:resource="#province"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="BelongTo">
    <rdfs:comment>تابعیت داشتن از</rdfs:comment>
    <rdfs:domain rdf:resource="#person"/><rdfs:domain rdf:resource="#space"/>
    <rdfs:range rdf:resource="#country"/><rdfs:range rdf:resource="#space"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="AffiliatedTo">
    <rdfs:comment>وابستگی سازمانی داشتن</rdfs:comment>
    <rdfs:domain rdf:resource="#HigherEducationOrg"/><rdfs:domain rdf:resource="#organization"/><rdfs:domain rdf:resource="#faculty"/>
    <rdfs:range rdf:resource="#HigherEducationOrg"/><rdfs:range rdf:resource="#organization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="LocatedAt">
    <rdfs:comment>قرار داشتن در</rdfs:comment>
    <rdfs:domain rdf:resource="#location"/><rdfs:domain rdf:resource="#OrganizationalDepartment"/><rdfs:domain rdf:resource="#space"/><rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#city"/><rdfs:range rdf:resource="#location"/><rdfs:range rdf:resource="#country"/><rdfs:range rdf:resource="#province"/><rdfs:range rdf:resource="#space"/><rdfs:range rdf:resource="#BuildingPart"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="RelatedTo">
    <rdfs:comment>مربوط بودن</rdfs:comment>
    <rdfs:domain rdf:resource="#OrganizationalPost"/><rdfs:domain rdf:resource="#EducationalCertificate"/><rdfs:domain rdf:resource="#EmploymentWrit"/><rdfs:domain rdf:resource="#StudyField"/><rdfs:domain rdf:resource="#StudentSemester"/><rdfs:domain rdf:resource="#SelectedLesson"/><rdfs:domain rdf:resource="#MagazineMember"/><rdfs:domain rdf:resource="#OrganizationMagazine"/><rdfs:domain rdf:resource="#AgreementMember"/><rdfs:domain rdf:resource="#ResearchPlanMember"/><rdfs:domain rdf:resource="#ThesisMember"/><rdfs:domain rdf:resource="#ThesisSupport"/><rdfs:domain rdf:resource="#PaperMember"/><rdfs:domain rdf:resource="#PatentMember"/><rdfs:domain rdf:resource="#BookMember"/><rdfs:domain rdf:resource="#EventPerson"/><rdfs:domain rdf:resource="#EventOrganization"/><rdfs:domain rdf:resource="#ScientificReward"/><rdfs:domain rdf:resource="#RoyaltyIncome"/><rdfs:domain rdf:resource="#ProductSale"/>
    <rdfs:range rdf:resource="#OrganizationalDepartment"/><rdfs:range rdf:resource="#JobField"/><rdfs:range rdf:resource="#StudyField"/><rdfs:range rdf:resource="#OrganizationalPost"/><rdfs:range rdf:resource="#EducationalGroup"/><rdfs:range rdf:resource="#Student"/><rdfs:range rdf:resource="#lesson"/><rdfs:range rdf:resource="#magazine"/><rdfs:range rdf:resource="#person"/><rdfs:range rdf:resource="#organization"/><rdfs:range rdf:resource="#ResearchPlan"/><rdfs:range rdf:resource="#agreement"/><rdfs:range rdf:resource="#thesis"/><rdfs:range rdf:resource="#paper"/><rdfs:range rdf:resource="#patent"/><rdfs:range rdf:resource="#book"/><rdfs:range rdf:resource="#EventInstance"/><rdfs:range rdf:resource="#event"/><rdfs:range rdf:resource="#royalty"/><rdfs:range rdf:resource="#product"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="SubSetOf">
    <rdfs:comment>زیر مجموعه بودن</rdfs:comment>
    <rdfs:domain rdf:resource="#OrganizationalPost"/>
    <rdfs:range rdf:resource="#OrganizationalPost"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="issued">
    <rdfs:comment>صادر کردن</rdfs:comment>
    <rdfs:domain rdf:resource="#HigherEducationOrg"/>
    <rdfs:range rdf:resource="#EducationalCertificate"/><rdfs:range rdf:resource="#EmploymentWrit"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="StudyAt">
    <rdfs:comment>تحصیل کردن در</rdfs:comment>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="#HigherEducationOrg"/><rdfs:range rdf:resource="#StudyField"/><rdfs:range rdf:resource="#EducationalGroup"/><rdfs:range rdf:resource="#faculty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="LiveAt">
    <rdfs:comment>ساکن بودن در</rdfs:comment>
    <rdfs:domain rdf:resource="#person"/>
    <rdfs:range rdf:resource="#province"/><rdfs:range rdf:resource="#city"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="take">
    <rdfs:comment>اخذ کردن</rdfs:comment>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="#SelectedLesson"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="HasBeen">
    <rdfs:comment>بودن در قبل</rdfs:comment>
    <rdfs:domain rdf:resource="#GraduatedStudent"/>
    <rdfs:range rdf:resource="#Student"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="present">
    <rdfs:comment>ارائه دادن</rdfs:comment>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="#thesis"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="presentedAt">
    <rdfs:comment>ارائه شدن در</rdfs:comment>
    <rdfs:domain rdf:resource="#paper"/>
    <rdfs:range rdf:resource="#event"/><rdfs:range rdf:resource="#MagazineIssue"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hold">
    <rdfs:comment>برگزار کردن</rdfs:comment>
    <rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#event"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="IsContractorOf">
    <rdfs:comment>پیمانکار بودن</rdfs:comment>
    <rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#ResearchContract"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="OrderedBy">
    <rdfs:comment>کارفرما بودن</rdfs:comment>
    <rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#ResearchContract"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="PaidFor">
    <rdfs:comment>پرداختن برای</rdfs:comment>
    <rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#RoyaltyIncome"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="ReceivedFrom">
    <rdfs:comment>دریافت کردن از</rdfs:comment>
    <rdfs:domain rdf:resource="#organization"/>
    <rdfs:range rdf:resource="#RoyaltyIncome"/><rdfs:range rdf:resource="#ProductSale"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="name">
    <rdfs:comment>نام</rdfs:comment>
    <rdfs:domain rdf:resource="#country"/><rdfs:domain rdf:resource="#province"/><rdfs:domain rdf:resource="#city"/><rdfs:domain rdf:resource="#person"/><rdfs:domain rdf:resource="#location"/><rdfs:domain rdf:resource="#space"/><rdfs:domain rdf:resource="#BuildingPart"/><rdfs:domain rdf:resource="#OrganizationalDepartment"/><rdfs:domain rdf:resource="#lesson"/><rdfs:domain rdf:resource="#StudyField"/><rdfs:domain rdf:resource="#faculty"/><rdfs:domain rdf:resource="#organization"/><rdfs:domain rdf:resource="#product"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="EnglishName">
    <rdfs:comment>نام انگلیسی</rdfs:comment>
    <rdfs:domain rdf:resource="#country"/><rdfs:domain rdf:resource="#province"/><rdfs:domain rdf:resource="#city"/><rdfs:domain rdf:resource="#organization"/><rdfs:domain rdf:resource="#product"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="LessonType">
    <rdfs:comment>نوع درس</rdfs:comment>
    <rdfs:domain rdf:resource="#lesson"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf>
          <rdf:List>
            <rdf:first rdf:datatype="&xsd;string">اختیاری</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first rdf:datatype="&xsd;string">اصلی</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first rdf:datatype="&xsd;string">پایان نامه</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first rdf:datatype="&xsd;string">پایه</rdf:first>
                        <rdf:rest>
                          <rdf:List>
                            <rdf:first rdf:datatype="&xsd;string">تخصصی</rdf:first>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:first rdf:datatype="&xsd;string">رساله</rdf:first>
                                <rdf:rest>
                                  <rdf:List>
                                    <rdf:first rdf:datatype="&xsd;string">سمینار</rdf:first>
                                    <rdf:rest>
                                      <rdf:List>
                                        <rdf:first rdf:datatype="&xsd;string">عمومی</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil" />
                                      </rdf:List>
                                    </rdf:rest>
                                  </rdf:List>
                                </rdf:rest>
                              </rdf:List>
                            </rdf:rest>
                          </rdf:List>
                        </rdf:rest>
                      </rdf:List>
                    </rdf:rest>
                  </rdf:List>
                </rdf:rest>
              </rdf:List>
            </rdf:rest>
          </rdf:List>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="BaseGroup">
    <rdfs:comment>گروه تحصیلی</rdfs:comment>
    <rdfs:domain rdf:resource="#StudyField"/><rdfs:domain rdf:resource="#product"/><rdfs:domain rdf:resource="#event"/><rdfs:domain rdf:resource="#ProductSale"/><rdfs:domain rdf:resource="#ScientificProduct"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf>
          <rdf:List>
            <rdf:first rdf:datatype="&xsd;string">دامپزشکی</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first rdf:datatype="&xsd;string">علوم اجتماعی</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first rdf:datatype="&xsd;string">علوم انسانی</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first rdf:datatype="&xsd;string">علوم پایه</rdf:first>
                        <rdf:rest>
                          <rdf:List>
                            <rdf:first rdf:datatype="&xsd;string">فنی و مهندسی</rdf:first>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:first rdf:datatype="&xsd;string">کشاورزی</rdf:first>
                                <rdf:rest>
                                  <rdf:List>
                                    <rdf:first rdf:datatype="&xsd;string">میان رشته ای</rdf:first>
                                    <rdf:rest>
                                      <rdf:List>
                                        <rdf:first rdf:datatype="&xsd;string">هنر و معماری</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil" />
                                      </rdf:List>
                                    </rdf:rest>
                                  </rdf:List>
                                </rdf:rest>
                              </rdf:List>
                            </rdf:rest>
                          </rdf:List>
                        </rdf:rest>
                      </rdf:List>
                    </rdf:rest>
                  </rdf:List>
                </rdf:rest>
              </rdf:List>
            </rdf:rest>
          </rdf:List>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="EducationalSection">
    <rdfs:comment>مقطع تحصیلی</rdfs:comment>
    <rdfs:domain rdf:resource="#EducationalCertificate"/><rdfs:domain rdf:resource="#StudyField"/><rdfs:domain rdf:resource="#Student"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf>
          <rdf:List>
            <rdf:first rdf:datatype="&xsd;string">دکتری تخصصی</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first rdf:datatype="&xsd;string">دکتری حرفه ای</rdf:first>
