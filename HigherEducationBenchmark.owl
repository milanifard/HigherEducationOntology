  <owl:Class rdf:ID="HigherEducationOrg">
    <rdfs:comment>موسسه آموزش عالی</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#organization"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#AffiliatedTo" />
        <owl:someValuesFrom rdf:resource="#HigherEducationOrg" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#issued" />
        <owl:someValuesFrom rdf:resource="#EducationalCertificate" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#issued" />
        <owl:someValuesFrom rdf:resource="#EmploymentWrit" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Student">
    <rdfs:comment>دانشجو</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#person"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#StudyAt" />
        <owl:someValuesFrom rdf:resource="#StudyField" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#StudyAt" />
        <owl:someValuesFrom rdf:resource="#EducationalGroup" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#StudyAt" />
        <owl:someValuesFrom rdf:resource="#HigherEducationOrg" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#StudyAt" />
        <owl:someValuesFrom rdf:resource="#faculty" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#take" />
        <owl:someValuesFrom rdf:resource="#SelectedLesson" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#present" />
        <owl:someValuesFrom rdf:resource="#thesis" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="magazine">
    <rdfs:comment>نشریه</rdfs:comment>
